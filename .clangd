CompileFlags:
  Add:
  - -I/Library/Developer/CommandLineTools/SDKs/MacOSX14.sdk/usr/include
  - -I/Library/Developer/CommandLineTools/SDKs/MacOSX14.sdk/System/Library/Frameworks
  - -DLOCAL
  - -Wall
  - -O2
  - -Wextra
  - -Wshadow
  - -Wconversion
  - -Wfloat-equal
  - -Wno-unused-const-variable
  - -Wno-sign-conversion
  - -std=c11
Index:
  Background: Skip
---
If:
  PathExclude: [.*\.c, .*\.h]
CompileFlags:
  Add:
  - -std=c++17
